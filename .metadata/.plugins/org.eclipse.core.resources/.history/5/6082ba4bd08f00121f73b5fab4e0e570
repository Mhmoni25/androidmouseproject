package itt.t00154755.mouseserver;


/**
 * @author Christopher
 * 
 * This is a utility class used to store some methods.
 * 
 */
public class ServerUtils {
	
	/**
	 * Use to output an error message to the screen.
	 * 
	 * @param tag 
	 * 		each class has a string tag to identify.
	 * @param e
	 * 		the Exception that is being thrown.
	 */
	public void error(String tag, Exception e){
		System.out.println(tag);
		e.printStackTrace();
	}
	
	/**
	 * 
	 * @param s
	 */
	public void info(String s){
		System.out.println(s);
	}
	
}
